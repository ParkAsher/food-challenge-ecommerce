<div class="login-background">
    <div class="login-wrap">
        <div class="login-form">
            <div class="login-title-wrap">
                <p>로그인</p>
            </div>
            <div class="login-email-wrap">
                <input id="email" class="login-email-input" placeholder="Email" />
            </div>
            <div class="login-password-wrap">
                <input id="password" class="login-password-input" placeholder="Password" />
            </div>
            <div class="error-message-wrap"></div>
            <div class="login-find-link-wrap">
                <div class="login-find-link-email-wrap">
                    <a href="/">이메일 찾기</a>
                </div>
                <div class="login-find-link-password-wrap">
                    <a href="/">비밀번호 찾기</a>
                </div>
            </div>
            <div class="login-btn-wrap">
                <button type="button" onclick="login()">로그인</button>
            </div>
            <div class="register-link-wrap">
                <a href="/register">아직 회원이 아니십니까?</a>
            </div>
        </div>
    </div>
</div>
