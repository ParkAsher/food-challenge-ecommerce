<div class="register-background">
    <div class="register-wrap">
        <div class="register-form">
            <div class="register-title-wrap">
                <p>회원가입</p>
            </div>
            <div class="register-name-wrap">
                <p>이름</p>
                <input id="name" class="register-name-input" placeholder="이름" />
            </div>
            <div class="register-nickname-wrap">
                <p>닉네임</p>
                <input id="nickname" class="register-nickname-input" placeholder="닉네임" />
            </div>
            <div class="register-email-wrap">
                <p>이메일</p>
                <input id="email" class="register-email-input" placeholder="이메일" />
            </div>
            <div class="register-password-wrap">
                <p>비밀번호</p>
                <input id="password" class="register-password-input" placeholder="비밀번호" />
            </div>
            <div class="register-password-check-wrap">
                <p>비밀번호확인</p>
                <input id="password-check" class="register-password-check-input" placeholder="비밀번호확인" />
            </div>
            <div class="register-btn-wrap">
                <button type="button">가입하기</button>
            </div>
        </div>
    </div>
    </div>
</div>
